# Jenkins Environment Variables
# Copy this file to .env for local testing or use as reference for Jenkins credentials

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESSKEY=JENKINS_WILL_INJECT_THIS
AWS_SECRETACCESSKEY=JENKINS_WILL_INJECT_THIS
S3_BUCKET_NAME=user-build-codes

# Kafka Configuration
KAFKA_BROKER=JENKINS_WILL_INJECT_THIS

# Project Configuration
PROJECT_ID=project-${BUILD_NUMBER}
DEPLOYMENT_ID=deployment-${BUILD_NUMBER}
GIT_URI=${GIT_URL}

# Build Configuration
BUILD_COMMAND=npm install && npm run build

# Deployment Verification
DEPLOYED_APP_URL=https://example.com/__outputs/${PROJECT_ID}/

# Resource Limits
MAX_CONCURRENT_UPLOADS=5 